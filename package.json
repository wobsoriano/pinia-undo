{
  "name": "pinia-undo",
  "version": "0.1.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wobsoriano/pinia-undo.git"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast --dts-resolve --minify --external vue",
    "test": "vitest run",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  },
  "dependencies": {
    "@ungap/structured-clone": "^1.0.1",
    "undo-stacker": "^0.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.32.0",
    "pinia": "^2.0.29",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.2",
    "vue": "^3.2.45"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "eslintIgnore": [
    "dist",
    "node_modules"
  ]
}
