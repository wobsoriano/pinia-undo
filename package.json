{
  "name": "pinia-undo",
  "version": "0.1.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wobsoriano/pinia-undo.git"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast --dts-resolve --minify --external vue",
    "test": "vitest run",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  },
  "dependencies": {
    "@ungap/structured-clone": "^1.2.0",
    "undo-stacker": "^0.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "pinia": "^2.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0",
    "vue": "^3.3.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "eslintIgnore": [
    "dist",
    "node_modules"
  ]
}
